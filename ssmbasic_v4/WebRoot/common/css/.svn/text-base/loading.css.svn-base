/*进度条*/
.ibody{
    width:100%;
    position:absolute;
    top:0px;
    left:0px;
}
.ol_loading {
    width: 100%;
    height:100px;
    background: url( '../images/loading.gif' );
    background-repeat: no-repeat;
    background-position: center center;
    display:none;
    position:absolute;
    top:50%;
    left:0px;
}
.ol_loading_mask{
    height:100%;
    width:100%;
    position:fixed;
    top:0px;
    left:0px;
    background-color: #000;
    opacity: 0.15;
    filter: alpha(opacity = 5);
    display:none;
}