package com.guoxin.module.system.condition;
import com.guoxin.common.page.BaseCondition;
import com.guoxin.module.system.entity.Role;

/**
 * 角色信息 Condition
 * @author Corey
 * @createTime 2016-12-03
 */
public class RoleCondition extends BaseCondition {
    private Role role;
    
    public RoleCondition() {
        this.role = new Role();
    }
    
    public RoleCondition(Role role) {
        this.role = role;
    }
    
    public Role getRole() {
        return role;
    }
    public void setRole(Role role) {
        this.role = role;
    }
}
